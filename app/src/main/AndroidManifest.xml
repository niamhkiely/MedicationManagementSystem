<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.medicationmanagementsystem">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="Medicate Health Care"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewPatientListContents"
            android:screenOrientation="landscape"/>
        <activity android:name=".AdminViewPatients" />
        <activity android:name=".ViewListContents"
            android:screenOrientation="landscape"/>
        <activity android:name=".NewPatient"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".Register" />
        <activity android:name=".NewCaringList"/>
        <activity android:name=".ViewCaringList"/>
        <activity android:name=".PatientsPerCaringList"/>
        <activity android:name=".UpdatePatient"/>
        <activity android:name=".AdminPatientsPerCL"/>
        <activity android:name=".NurseViewCaringList"/>
        <activity android:name=".PrescriptionsPerPatient"/>
        <activity android:name=".DoctorHome"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".NurseHome"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".AdminHome"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".SignatureActivity" />

    </application>

</manifest>